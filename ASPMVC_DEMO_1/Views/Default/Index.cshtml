@model ASPMVC_DEMO_1.Models.Product
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>
</head>
<body>
<div>
    <h1>@ViewBag.Title</h1>
    <div>@ViewBag.Content</div>
    <h2>Demo product:</h2>
    <ul>
        <li>Id: @Model.Id</li>
        <li>Name: @Model.Name</li>
        <li>Price: @Model.Price</li>
    </ul>
</div>
</body>
</html>